<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="factory.controller.UsersController">
    <TableView fx:id="usersTableView" prefHeight="${anchorPane.height}" prefWidth="${anchorPane.width}">
        <columns>
            <TableColumn fx:id="usernameColumn" prefWidth="100.0" text="Username" />
            <TableColumn fx:id="companyColumn" prefWidth="140.0" text="Company" />
            <TableColumn fx:id="phoneColumn" minWidth="0.199981689453125" prefWidth="100.0" text="Phone" />
            <TableColumn fx:id="emailColumn" prefWidth="200.0" text="E-mail" />
         <TableColumn fx:id="statusColumn" prefWidth="75.0" text="Status" />
            <TableColumn fx:id="acceptColumn" minWidth="0.0" prefWidth="60.0" />
            <TableColumn fx:id="rejectColumn" prefWidth="60.0" />
        </columns>
    </TableView>
</AnchorPane>
